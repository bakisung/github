package GameProject;

public class training {
	
	// 선언
	String name;
	int cm;
	int kg;
	int age;
	String sex;
	String hobby;
	
	// 인물
	void 박희성() {
		cm = 180;
		kg = 42;
	}
	
	void 박희성2() {
		cm = 180;
		kg = 42;
	}
	void 박희성3() {
		cm = 180;
		kg = 42;
	}
	
	public static void main(String[] args) {
		
		training tr = new training();
		
		tr.박희성();
		System.out.println(tr.cm);
	}

}
