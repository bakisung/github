package f_oop2;

public class Inheritance {

	class Human {
		String name;
		int age;
		void eat() {};
		void sleep() {};
	}
	
	class Student extends Human {
		int studentID;
		void goToSchool() {};
	}
	
	class Worker extends Human {
		int workerID;
		void goToWork() {};
	}
	
	
	
	// 메인
	public static void main(String[] args) {

	} // 메인 종료

} // 클래스 종료
