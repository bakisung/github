package e_oop.score;

public class Student {

	int kor;
	int eng;
	int math;
	int sum;
	double avg;
	String name;
	int rank;
	
	Student(int i) {
		name = "학생" + (i + 1);
		rank = 1;
		kor = (int)(Math.random() * 100) + 1;
		eng = (int)(Math.random() * 100) + 1;
		math = (int)(Math.random() * 100) + 1;
	}
	
}
