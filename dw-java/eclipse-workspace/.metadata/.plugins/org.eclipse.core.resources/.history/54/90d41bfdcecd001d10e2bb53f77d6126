package b_operator;

public class ComparisonOperator {

	public static void main(String[] args) {
		
		/*
		 * << 비교 연산자 >>
		 *  - < : 작다
		 *  - > : 크다
		 *  - <= : 작거나 같다
		 *  - >= : 크거나 같다
		 *  - == : 같다
		 *  - != : 같지 않다
		 */
		
		boolean b = 10 < 20;
		System.out.println(b);
		
		
		
		
		
		
		
		
		
		

	}

}
