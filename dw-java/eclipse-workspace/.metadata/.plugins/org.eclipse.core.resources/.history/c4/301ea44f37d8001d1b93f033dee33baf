package GameProject;

public class training {
	
	// 선언
	String name;
	int cm;
	int kg;
	int age;
	String sex;
	String hobby;
	
	// 인물
	void 박희성() {
		cm = 180;
		System.out.println(cm);
		return;
	}
	
	public static void main(String[] args) {
		
		training tr = new training();
		
		String 키 = 박희성();
		
	}

}
