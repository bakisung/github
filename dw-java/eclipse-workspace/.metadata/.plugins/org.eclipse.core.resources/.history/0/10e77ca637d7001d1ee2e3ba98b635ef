		items = new Item[10];
		items[0] = new Item("무한의 대검",0, 0, 10, 0);
		items[1] = new Item("가시갑옷", 0, 0, 0, 10);
	}
	
	
	
	
	
	
	
	
	public static void main(String[] args) {

		new MyGame2().start();
		
	} // main 종료








	private void start() {
		Scanner sc = new Scanner(System.in);
		while (true) {
			System.out.println("1.내정보/t2.사냥/t3.종료");
			int input = 0;
		}
	}










	

} // class 종료
